                        </Record>
                        <Record Type="com.sap.vocabularies.UI.v1.DataField">
                            <PropertyValue Property="Value" Path="InventoryNumber"/>
                        </Record>
                        <Record Type="com.sap.vocabularies.UI.v1.DataField">
